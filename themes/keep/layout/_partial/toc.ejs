<div class="post-toc-wrap border-box">
    <div class="post-toc border-box">
        <!-- use hexo-blog-encrypt -->
        <%- toc(
            page?.encrypt === true ? page.origin : page.content,
            {
                class: 'nav',
                list_number: theme?.toc?.number || false
            }
        ) %>
    </div>
</div>
